####################################################################################################
# GENERIC
####################################################################################################

run: build_${TEST}_test
	@build/${TEST}_test.exe

.PHONY: build_${TEST}_test
build_${TEST}_test:
	@mkdir -p build
	@gcc -I./src test/${TEST}_test.c -o build/${TEST}_test.exe

clean:
	@rm -rf *.exe build dpi

####################################################################################################
# SPECIFIC
####################################################################################################
